{
  "name": "vue-owl-ui",
  "version": "0.1.0",
  "description": "Cepave OWL UI Components for VueJS 2.0",
  "main": "./dist/owl-ui",
  "scripts": {
    "start": "babel-node scripts/start",
    "test": "npm run lint && jest src/ --no-cache --coverage",
    "dev": "babel-node ./scripts/dev",
    "build": "npm run build.ui & npm run build.demo",
    "build.ui": "NODE_ENV=build webpack --progress --config webpack/client && babel-node ./scripts/build.ui",
    "build.demo": "NODE_ENV=production webpack --progress --config webpack/client",
    "lint": "eslint src & stylelint src/**/*.scss --syntax scss",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "jest": {
    "verbose": false,
    "moduleNameMapper": {
      "^.+\\.(sc?ss|svg|png|jpg|gif)$": "<rootDir>/test/mock-loader"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.s?css$"
    ],
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "keywords": [
    "design",
    "system",
    "vue",
    "component",
    "ui",
    "owl"
  ],
  "author": "Cepave F2E Team",
  "license": "MIT",
  "dependencies": {
    "delegate-to": "^0.1.2",
    "vue": "^2.0.0-rc.8"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-vue-jsx": "^3.1.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-style-loader": "^1.0.0",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.10.0",
    "normalize.css": "^4.2.0",
    "piping": "^1.0.0-rc.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "vue-router": "^2.0.0-rc.7",
    "vue-server-renderer": "^2.0.0-rc.8",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cepave-f2e/vue-owl-ui.git"
  },
  "bugs": {
    "url": "https://github.com/cepave-f2e/vue-owl-ui/issues"
  },
  "homepage": "https://github.com/cepave-f2e/vue-owl-ui#readme"
}
